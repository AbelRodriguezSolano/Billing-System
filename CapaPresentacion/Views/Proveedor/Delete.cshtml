@model CapaEntidad.Proveedore

@{
    ViewBag.Title = "Delete";
}

<h2 class="mt-3 text-center text-uppercase">¿Está seguro de eliminar este Proveedor?</h2>


<div class="row justify-content-center mt-5">
    <div class="col-12 col-md-8 text-center">
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions">
                <input type="submit" value="Eliminar" class="btn btn-danger btn-block mb-2" />
                <a href="/Proveedor/Index" class="btn btn-outline-dark btn-block">Volver</a>
            </div>
        }
    </div>
</div>